<odoo>
    <template id="lib_inv_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Libro de Inventario</h2>
                        <p>Fecha Inicio: <t t-esc="doc.date_from"/></p>
                        <p>Fecha Fin: <t t-esc="doc.date_to"/></p>
                        </div>
                </t>
            </t>
        </t>
    </template>
</odoo>